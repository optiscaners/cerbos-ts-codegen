{
	"name": "@estino/cerbos-ts-codegen",
	"version": "0.0.2",
	"description": "codegen for cerbos policy schemas",
	"keywords": [
		"cerbos",
		"authorization",
		"codegen",
		"typescript"
	],
	"packageManager": "pnpm@8.6.0",
	"repository": "github:optiscaners/cerbos-ts-codegen",
	"type": "module",
	"main": "dist/index.js",
	"files": [
		"dist"
	],
	"bin": {
		"@estino/cerbos-ts-codegen": "dist/index.js"
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "tsc",
		"dev": "node --watch --loader ts-node/esm --experimental-specifier-resolution=node src/dev.ts",
		"dev:prompt": "node --watch --loader ts-node/esm --experimental-specifier-resolution=node src/index.ts",
		"format": "prettier --write ."
	},
	"author": "Manuel DugueÃÅ",
	"license": "MIT",
	"homepage": "https://www.estino.de/",
	"dependencies": {
		"@cerbos/core": "^0.11.0",
		"@cerbos/http": "^0.12.0",
		"@inquirer/prompts": "^2.1.1",
		"@types/node": "^20.3.1",
		"camelcase": "^7.0.1",
		"chalk": "^5.2.0",
		"js-yaml": "^4.1.0",
		"json-schema-to-typescript": "^13.0.2",
		"prettier": "^2.8.8",
		"ts-morph": "^18.0.0",
		"typescript": "^5.1.3",
		"zod": "^3.21.4"
	},
	"devDependencies": {
		"@types/js-yaml": "^4.0.5",
		"ts-node": "^10.9.1"
	}
}
